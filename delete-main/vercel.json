{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/services",
      "dest": "/services.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/help",
      "dest": "/help.html"
    },
    {
      "src": "/rating",
      "dest": "/rating.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
